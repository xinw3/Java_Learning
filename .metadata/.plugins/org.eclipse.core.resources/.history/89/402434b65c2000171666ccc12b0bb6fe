package cloudera;

public class BST {

	public class TreeNode<T> {
		T val;
		TreeNode<T> left, right;
		public TreeNode (T value) {
			val = value;
			left = null;
			right = null;
		}
	}
	public <T> T RecTraversal(TreeNode<T> root) {
		T value = 0;
		if(root != null) {
			T left = RecTraversal(root.left);
			value = root.val;
			T right = RecTraversal(root.right);
			return value;
		}
		return value;
	}
	public static void main(String[] args) {
		

	}

}
