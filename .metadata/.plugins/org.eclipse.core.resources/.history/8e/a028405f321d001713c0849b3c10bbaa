package demo2;

class Processor extends Thread {
	public void run() {
		while (true) {
			System.out.println("Hello!");
			
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}
public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
