package threadSafe;

public class SyncApp {

	private int count = 0;
	
	public static void main(String[] args) {
		SyncApp app = new SyncApp();
		app.doWork();
	}
	public void doWork() {
		Thread t1 = new Thread(new Runnable() {
			public void run() {
				for (int i = 0; i < 10000; i++) {
					count++;
				}
			}
		});
		
		Thread t2 = new Thread(new Runnable() {
			public void run() {
				for (int i = 0; i < 10000; i++) {
					count++;
				}
			}
		});
	}

}
